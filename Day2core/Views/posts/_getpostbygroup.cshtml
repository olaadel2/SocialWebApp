@model IEnumerable<Day2core.Models.post>

@*<a class="btn btn-info" onclick="adddata()">create</a>*@


<h1>posts</h1>
<div></div>
<table class="table">
    <tr>
        <th>id</th>
        <th>title</th>
        <th>bref</th>
        <th>date</th>
        @*<th>blog</th>*@
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.id</td>
            <td>@item.title</td>
            <td>@item.bref</td>
            <td>@item.date</td>
            @*<td>@item.Blog.name</td>*@
           
        </tr>
    }

</table>

